{
    "manifest_version": 3,
    "name": "Color Contrast Checker",
    "version": "1.0.0",
    "description": "A simple color contrast check with a popup.",
    "author": "Novalexdroid",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon16.png"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }